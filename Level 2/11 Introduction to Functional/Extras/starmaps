import itertools

def f(x, y, z):
    print(x, y, z)

def g(x):
    print(x)

# starmap binds an iterable of muliple parameters to a function
it = itertools.starmap(f, [[20, 21, 22], [30, 31, 32], [40, 41, 42]])
next(it)
next(it)
next(it)

# map binds an iterable of single parameters to a function
it = map(g, [20, 30, 40])
next(it)
next(it)
next(it)
